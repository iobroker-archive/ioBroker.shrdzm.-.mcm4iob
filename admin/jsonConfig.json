{
    "i18n": true,
    "type": "tabs",
    "items": {
        "_stdConfig": {
            "type": "panel",
            "label": "lblStdConfig",
            "items": {
                "_header_1": {
                    "type": "header",
                    "text": "lblHdr_1",
                    "size": 3,
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12                   
                },
                "port": {
                    "type": "port",
                    "label": "lblPort",
                    "newLine": true,
                    "min": 1,
                    "max": 65535,
                    "default": 9000,
                    "xs": 12,
                    "sm": 8,
                    "md": 4,
                    "lg": 2,
                    "xl": 2
                },
                "devices": {
                    "type": "chips",
                    "label": "lblDevices",
                    "newLine": true,
                    "default": "",
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12
                },
                "_header_2": {
                    "type": "header",
                    "text": "lblHdr_2",
                    "size": 3,
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12                   
                },
                "energyRate":{
                    "type": "select",
                    "label": "lblEnergyRate",
                    "options": [
                        {"label": "none", "value": 0},
                        {"label": "max", "value": 1},
                        {"label": "1:2", "value": 2},
                        {"label": "1:5", "value": 5},
                        {"label": "1:10", "value": 10},
                        {"label": "1:15", "value": 15},
                        {"label": "1:30", "value": 30}                                                
                    ],
                    "default": 15,
                    "xs": 12,
                    "sm": 8,
                    "md": 4,
                    "lg": 2,
                    "xl": 2                   
                },
                "powerRate":{
                    "type": "select",
                    "label": "lblPowerRate",
                    "options": [
                        {"label": "none", "value": 0},
                        {"label": "max", "value": 1},
                        {"label": "1:2", "value": 2},
                        {"label": "1:5", "value": 5},
                        {"label": "1:10", "value": 10},
                        {"label": "1:15", "value": 15},
                        {"label": "1:30", "value": 30}                                                
                    ],
                    "default": 5,
                    "xs": 12,
                    "sm": 8,
                    "md": 4,
                    "lg": 2,
                    "xl": 2                   
                }
            }
        },
        "_expertConfig": {
            "type": "panel",
            "label": "lblExpertConfig",
            "expertMode": true,
            "items": {
                "storeRawData": {
                    "type": "checkbox",
                    "label": "lblStoreRawData",
                    "newLine": true,
                    "expertMode": true,
                    "default": false,
                    "xs": 12,
                    "sm": 12,
                    "md": 10,
                    "lg": 8,
                    "xl": 4
                },
                "udpFwdEnable": {
                    "type": "checkbox",
                    "label": "lblUdpFwdEnable",
                    "newLine": true,
                    "expertMode": true,
                    "default": false,
                    "xs": 12,
                    "sm": 12,
                    "md": 10,
                    "lg": 8,
                    "xl": 4
                },
                "udpFwdAddress": {
                    "type": "text",
                    "label": "lblUdpFwdAddress",
                    "newLine": true,
                    "hidden": "!data.udpFwdEnable",
                    "expertMode": true,
                    "default": "",
                    "xs": 12,
                    "sm": 10,
                    "md": 8,
                    "lg": 6,
                    "xl": 4
                },
                "udpFwdPort": {
                    "type": "number",
                    "label": "lblUdpFwdPort",
                    "newLine": false,
                    "expertMode": true,
                    "hidden": "!data.udpFwdEnable",
                    "default": 9000,
                    "min": 1,
                    "max": 65535,
                    "xs": 12,
                    "sm": 10,
                    "md": 6,
                    "lg": 4,
                    "xl": 2
                }                
            }
        }
    }
}